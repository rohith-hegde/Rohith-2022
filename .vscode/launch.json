	"version": "0.2.0",
	"configurations": [
		{
			"type": "pwa-node",
			"request": "launch",
			"name": "Launch server",
			"skipFiles": ["<node_internals>/**"],
			"program": "${workspaceFolder}\\server.js",
			"args": ["openSesame123"]
		},
		{
			"type": "pwa-node",
			"request": "launch",
			"name": "Launch client",
			"skipFiles": ["<node_internals>/**"],
			"program": "${workspaceFolder}\\client.js",
			"args": ["openSesame123"]
		}
	]
}
